 <script>
 
 function generateQRCode(data) {
     const qrContainer = document.getElementById("qrCodePlaceholder");
     const qrSize = 100;  // QR code size (width and height)
     const xPosition = 540; // X-coordinate to position the QR code inside the SVG
     const yPosition = 145; // Y-coordinate to position the QR code inside the SVG

     // Generate the SVG QR code
     QRCode.toString(data, {
         type: 'svg',
         width: qrSize,    // Set width of the QR code
         margin: 2         // Set margin (padding) around the QR code
     }, function (error, svg) {
         if (error) {
             console.error("QR Code generation error:", error);
         } else {
             // Parse the generated SVG QR code and insert it into the main SVG
             const svgElement = new DOMParser().parseFromString(svg, "image/svg+xml").documentElement;
             svgElement.setAttribute('x', xPosition); // Set the X position
             svgElement.setAttribute('y', yPosition); // Set the Y position
             svgElement.setAttribute('width', qrSize); // Set the width
             svgElement.setAttribute('height', qrSize); // Set the height

             // Append the generated QR code into the placeholder group
             qrContainer.innerHTML = '';  // Clear previous QR code (if any)
             qrContainer.appendChild(svgElement);  // Insert the new QR code
             console.log("QR Code generated and embedded inside the SVG.");
         }
     });
 }

        // Ticket data to encode in the QR code
        const ticketData = `
        	 ${sessionScope.movieTitle}+","+
             ${sessionScope.theatreName}+","+
            ${sessionScope.showDate}+","+
            ${sessionScope.showtime}+","+
            ${sessionScope.selectedSeats}+","+
            ${sessionScope.totalAmount}
        `.trim();

        // Generate the QR Code
        generateQRCode(ticketData);
    </script>


<script>
    function saveAsJPG() {
      // Get the SVG element
      const svg = document.getElementById("mySVG");
      
  
      // Convert SVG to XML string
      const svgData = new XMLSerializer().serializeToString(svg);
  
      // Create an image and set its source to the SVG string
      const img = new Image();
      const svgBlob = new Blob([svgData], { type: "image/svg+xml;charset=utf-8" });
      const url = URL.createObjectURL(svgBlob);
  
      img.onload = function () {
        // High-Quality Scale Factor
        const SCALE_FACTOR = 4; // Increase for higher resolution (e.g., 2, 3, 4)
  
        // Set up a high-resolution canvas
        const canvas = document.createElement("canvas");
        const width = svg.clientWidth;
        const height = svg.clientHeight;
  
        canvas.width = width * SCALE_FACTOR;
        canvas.height = height * SCALE_FACTOR;
  
        // Scale the context to draw high-resolution content
        const ctx = canvas.getContext("2d");
        ctx.scale(SCALE_FACTOR, SCALE_FACTOR);
  
        // Draw the image onto the canvas
        ctx.drawImage(img, 0, 0, width, height);
  
        // Convert the canvas to a high-quality JPG
        const jpgURL = canvas.toDataURL("image/jpeg", 1.0); // 1.0 = 100% quality
  
        // Trigger a download of the JPG
        const a = document.createElement("a");
        a.href = jpgURL;
        a.download = "high-quality-image.jpg";
        a.click();
  
        // Cleanup
        URL.revokeObjectURL(url);
      };
  
      img.src = url;
    }
    
  </script>

  
